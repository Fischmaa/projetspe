library("dplyr")
library("BBmisc")
library(e1071)
library(class)
library("data.table")

train<-fread('../train.csv',header=TRUE)
train_cluster<-train$hotel_cluster
train<-train %>% BBmisc::dropNamed("hotel_cluster")

classifier<-naiveBayes(train,train_cluster)

test<-fread('../test.csv',header=TRUE)
res<-predict(classifier,test)
